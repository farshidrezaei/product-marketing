APP_NAME=product_marketing
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost
APP_PORT=8080

LOG_CHANNEL=daily
LOG_DEPRECATIONS_CHANNEL=null
