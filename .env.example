DB_CONNECTION=mysql
DB_HOST=db
DB_PORT=3306
DB_DATABASE=product_marketing_db
DB_USERNAME=product_marketing_user
DB_PASSWORD=password
DB_ROOT_PASSWORD=root_password

BROADCAST_DRIVER=log
